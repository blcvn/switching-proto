    "accesspointv1Account": {
      "type": "object",
      "properties": {
        "iban": {
          "type": "string",
          "title": "International Bank Account Number (if available)"
        },
        "account_number": {
          "type": "string",
          "title": "Local account number"
        },
        "account_name": {
          "type": "string",
          "title": "Account holder name"
        },
        "currency": {
          "type": "string",
          "title": "Account currency (optional)"
        },
        "reference": {
          "type": "string",
          "title": "Additional reference or identification (e.g., secondary id)"
        }
      },
      "description": "Account represents the account details associated with an agent."
    },
