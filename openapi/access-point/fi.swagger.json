        },
        "status": {
          "type": "string",
          "title": "Trạng thái chuyển tiền"
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "title": "Thời gian tạo lệnh chuyển"
        },
        "creditor_account": {
          "$ref": "#/definitions/accesspointv1Account",
          "title": "Thông tin tài khoản chi tiết của người nhận trong chuyển tiền FI (tùy chọn)"
        }
      },
      "title": "Thông tin chuyển tiền (transfer) giữa các tổ chức tài chính"
    },
    "accesspointv1TransferStatus": {
      "type": "object",
      "properties": {
        "metadata": {
          "$ref": "#/definitions/accesspointv1Metadata",
          "title": "Thông tin metadata của response"
        },
        "signature": {
          "$ref": "#/definitions/accesspointv1Signature",
          "title": "Chữ ký số của response"
        },
        "result": {
          "$ref": "#/definitions/accesspointv1Result",
          "title": "Kết quả xử lý"
        },
        "data": {
          "$ref": "#/definitions/accesspointv1Transfer",
