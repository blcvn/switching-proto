        "currency": {
          "type": "string",
          "title": "Lọc theo mã tiền tệ"
        },
        "date_from": {
          "type": "string",
          "title": "Ngày bắt đầu (từ ngày)"
        },
        "date_to": {
          "type": "string",
          "title": "Ngày kết thúc (đến ngày)"
        }
      },
      "title": "Bộ lọc tìm kiếm"
    },
    "accesspointv1SearchTransfersResponse": {
      "type": "object",
