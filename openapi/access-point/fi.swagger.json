        },
        "account_number": {
          "type": "string",
          "title": "Số tài khoản nội địa"
        },
        "account_name": {
          "type": "string",
          "title": "Tên chủ tài khoản"
        },
        "currency": {
          "type": "string",
          "title": "Mã tiền tệ của tài khoản (tùy chọn)"
        },
        "reference": {
          "type": "string",
          "title": "Thông tin tham chiếu hoặc định danh bổ sung (ví dụ: ID phụ)"
        }
      },
      "title": "Thông tin chi tiết tài khoản liên kết với một agent"
    },
    "accesspointv1BulkTransferResponse": {
