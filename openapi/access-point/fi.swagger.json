        }
      },
      "title": "Response tìm kiếm lệnh chuyển tiền"
    },
    "accesspointv1Signature": {
      "type": "object",
      "properties": {
        "s_type": {
          "$ref": "#/definitions/accesspointv1SignatureSignatureType",
          "title": "Loại chữ ký được sử dụng"
        },
        "s": {
          "type": "string",
          "title": "Chuỗi dữ liệu dùng để tạo ra chữ ký"
        },
        "b": {
          "type": "string",
          "format": "byte",
          "title": "Chữ ký số (dạng bytes)"
        }
      },
      "title": "Signature định nghĩa cấu trúc chữ ký số cho request/response"
    },
    "accesspointv1SignatureSignatureType": {
      "type": "string",
      "enum": [
        "NO_USE_TYPE",
        "J",
        "C",
        "S"
      ],
      "default": "NO_USE_TYPE",
      "description": "- NO_USE_TYPE: Giá trị mặc định, không sử dụng\n - J: Chữ ký loại J\n - C: Chữ ký loại C\n - S: Chữ ký loại S",
      "title": "SignatureType định nghĩa các loại chữ ký được hỗ trợ"
    },
    "accesspointv1Transfer": {
      "type": "object",
      "properties": {
        "instruction_id": {
          "type": "string",
          "title": "ID của lệnh chuyển tiền"
