          "$ref": "#/definitions/offchainv1Signature",
          "title": "Chữ ký số của response"
        },
        "result": {
          "$ref": "#/definitions/offchainv1Result",
          "title": "Kết quả thực thi"
        },
        "data": {
          "$ref": "#/definitions/offchainv1Payment",
          "title": "Dữ liệu thanh toán đã xác nhận"
        }
      },
      "title": "Phản hồi xác nhận thanh toán"
    },
    "offchainv1CreatePaymentResponse": {
      "type": "object",
      "properties": {
        "metadata": {
          "$ref": "#/definitions/offchainv1Metadata",
          "title": "Metadata của response"
        },
        "signature": {
          "$ref": "#/definitions/offchainv1Signature",
          "title": "Chữ ký số của response"
        },
        "result": {
          "$ref": "#/definitions/offchainv1Result",
          "title": "Kết quả thực thi"
        },
        "data": {
          "$ref": "#/definitions/offchainv1Payment",
          "title": "Dữ liệu thanh toán đã tạo"
        }
      },
      "title": "Phản hồi tạo thanh toán"
    },
    "offchainv1GetPaymentResponse": {
      "type": "object",
      "properties": {
        "metadata": {
          "$ref": "#/definitions/offchainv1Metadata",
          "title": "Metadata của response"
        },
        "signature": {
          "$ref": "#/definitions/offchainv1Signature",
          "title": "Chữ ký số của response"
        },
        "result": {
          "$ref": "#/definitions/offchainv1Result",
          "title": "Kết quả thực thi"
        },
        "data": {
          "$ref": "#/definitions/offchainv1Payment",
          "title": "Dữ liệu thanh toán"
        }
      },
      "title": "Phản hồi lấy thông tin thanh toán"
    },
    "offchainv1Metadata": {
      "type": "object",
      "properties": {
        "request_id": {
          "type": "string",
          "title": "ID định danh duy nhất cho request"
        },
        "request_time": {
          "type": "string",
          "format": "int64",
          "title": "Thời gian gửi request (Unix timestamp)"
        },
        "version": {
          "type": "string",
          "title": "Phiên bản của protocol"
        }
      }
    },
    "offchainv1Payment": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "title": "ID định danh thanh toán"
        },
        "uetr": {
          "type": "string",
          "title": "Unique End-to-End Transaction Reference"
        },
        "amount": {
          "type": "number",
          "format": "double",
          "title": "Số tiền thanh toán"
        },
        "currency": {
          "type": "string",
          "title": "Mã tiền tệ (ví dụ: USD, VND)"
        },
        "debtor_agent": {
          "type": "string",
          "title": "BIC của ngân hàng người gửi"
        },
        "creditor_agent": {
          "type": "string",
          "title": "BIC của ngân hàng người nhận"
        },
        "end_to_end_id": {
          "type": "string",
          "title": "ID định danh end-to-end của giao dịch"
        },
        "status": {
          "type": "string",
          "title": "Trạng thái thanh toán"
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "title": "Thời gian tạo thanh toán"
        },
        "debtor_agent_account": {
          "$ref": "#/definitions/offchainv1Account",
          "title": "Thông tin tài khoản chi tiết của ngân hàng người gửi (tùy chọn)"
        },
        "creditor_agent_account": {
          "$ref": "#/definitions/offchainv1Account",
          "title": "Thông tin tài khoản chi tiết của ngân hàng người nhận (tùy chọn)"
        },
        "transaction_reference": {
          "type": "string",
          "title": "Mã tham chiếu giao dịch"
        }
      },
      "title": "Thông tin thanh toán"
    },
    "offchainv1Result": {
      "type": "object",
      "properties": {
        "code": {
          "$ref": "#/definitions/offchainv1ResultCode",
          "title": "Mã kết quả thực thi (enum)"
        },
        "message": {
          "type": "string",
          "title": "Thông điệp mô tả kết quả"
        }
      },
      "title": "Result định nghĩa cấu trúc kết quả trả về"
    },
    "offchainv1ResultCode": {
      "type": "string",
      "enum": [
        "UNSPECIFIED",
        "SUCCESS",
        "BAD_REQUEST",
        "UNAUTHORIZED",
        "FORBIDDEN",
        "NOT_FOUND",
        "DUPLICATE_REQUEST",
        "UNPROCESSABLE_ENTITY",
        "TOO_MANY_REQUESTS",
        "INTERNAL",
        "BAD_GATEWAY",
        "SERVICE_UNAVAILABLE",
        "GATEWAY_TIMEOUT"
      ],
      "default": "UNSPECIFIED",
      "description": "- UNSPECIFIED: Giá trị mặc định, không xác định\n - SUCCESS: Thành công\n - BAD_REQUEST: Lỗi validation hoặc request không hợp lệ\n - UNAUTHORIZED: Chưa xác thực hoặc token không hợp lệ\n - FORBIDDEN: Không có quyền truy cập\n - NOT_FOUND: Không tìm thấy tài nguyên\n - DUPLICATE_REQUEST: Request trùng lặp\n - UNPROCESSABLE_ENTITY: Dữ liệu không thể xử lý\n - TOO_MANY_REQUESTS: Quá nhiều request trong khoảng thời gian\n - INTERNAL: Lỗi nội bộ của server\n - BAD_GATEWAY: Lỗi gateway\n - SERVICE_UNAVAILABLE: Dịch vụ không khả dụng\n - GATEWAY_TIMEOUT: Gateway timeout",
      "title": "ResultCode định nghĩa các mã kết quả được sử dụng trong hệ thống"
    },
    "offchainv1Signature": {
      "type": "object",
      "properties": {
        "s_type": {
          "$ref": "#/definitions/offchainv1SignatureSignatureType",
          "title": "Loại chữ ký được sử dụng"
        },
        "s": {
          "type": "string",
          "title": "Chuỗi dùng tạo ra chữ ký"
        },
        "b": {
          "type": "string",
          "format": "byte",
          "title": "Chữ ký số"
        }
      },
      "title": "CCSignature định nghĩa cấu trúc chữ ký số cho request"
    },
    "offchainv1SignatureSignatureType": {
      "type": "string",
      "enum": [
        "NO_USE_TYPE",
        "J",
        "C",
        "S"
      ],
      "default": "NO_USE_TYPE",
      "description": "- NO_USE_TYPE: Giá trị mặc định, không sử dụng\n - J: Chữ ký loại J\n - C: Chữ ký loại C\n - S: Chữ ký loại S",
      "title": "SignatureType định nghĩa các loại chữ ký được hỗ trợ"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
