  "paths": {},
  "definitions": {
    "offchainv1Account": {
      "type": "object",
      "properties": {
        "iban": {
          "type": "string",
          "title": "Số tài khoản quốc tế IBAN (nếu có)"
        },
        "account_number": {
          "type": "string",
          "title": "Số tài khoản nội địa"
        },
        "account_name": {
          "type": "string",
          "title": "Tên chủ tài khoản"
        },
        "currency": {
          "type": "string",
          "title": "Mã tiền tệ của tài khoản (tùy chọn)"
        },
        "reference": {
          "type": "string",
          "title": "Thông tin tham chiếu hoặc định danh bổ sung (ví dụ: ID phụ)"
        }
      },
      "title": "Thông tin chi tiết tài khoản liên kết với một agent"
    },
    "offchainv1ConfirmMessage": {
      "type": "object",
      "properties": {
        "uetr": {
          "type": "string",
          "title": "Unique End-to-End Transaction Reference"
        },
        "confirmation_type": {
          "type": "string",
          "title": "Loại xác nhận"
        },
        "timestamp": {
          "type": "string",
          "title": "Thời gian xác nhận"
        }
      },
      "title": "Thông điệp xác nhận thanh toán"
    },
    "offchainv1ConfirmPaymentResponse": {
      "type": "object",
      "properties": {
