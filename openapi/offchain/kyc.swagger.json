    "/kyc/detail": {
      "post": {
        "summary": "Lấy thông tin chi tiết của một thực thể KYC",
        "operationId": "KycService_GetEntity",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1KycEntity"
            }
          },
          "default": {
